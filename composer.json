{
	"name": "stolz/market",
	"description": "Market Trade Processor",
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/lumen-framework": "~5.1.4",
		"vlucas/phpdotenv": "~1.0",
		"stolz/laravel-form-builder": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"squizlabs/php_codesniffer": "~2"
	},
	"autoload": {
		"files": [
			"resources/helpers.php"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"database/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"config": {
		"discard-changes": true,
		"optimize-autoloader": true,
		"preferred-install": "dist"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
